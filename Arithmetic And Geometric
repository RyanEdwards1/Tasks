#include <iostream> /*Libary*/
#include <time.h>


using namespace std;

int main () {
  
int input;

cout << "Enter a number" << "\n";

cin >> input;

int geometric = input + input;

int geometric2 = geometric + input;

int geometric3 = geometric2 + input;

cout << "Geometric Number: " "\n";
cout << geometric << "\n";
cout << geometric2 << "\n";
cout << geometric3 << "\n";
}
